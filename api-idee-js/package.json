  },
  "overrides": {
    "webpack": "5.88.2",
    "eslint": "8.51.0"
  },
  "scripts": {
    "build": "./node/npm run build:core && ./node/npm run build:plugins",
    "build:sourcemap": "npm run prebuild && npm run build:core -- --source-map && npm run build:plugins",
    "build:core": "webpack --config=webpack-config/webpack.production-core.config.js",
    "build:plugins": "node tasks/build-plugins",
    "debug:build": "webpack serve --config=webpack-config/webpack.test-production.config.js",
    "debug:prebuild": "node --inspect-brk tasks/generate-index.js",
    "generate-doc": "jsdoc config/jsdoc/api/index.md -c config/jsdoc/api/conf.json",
    "generate-test": "node tasks/test-generator",
    "karma": "karma start ./karma.conf.js",
    "prebuild": "node tasks/clean-build.js --path=dist && node tasks/generate-index.js",
    "postbuild": "npm run generate-doc && node tasks/copy-legacy.js",
    "start-cnig": "webpack-dev-server --config=webpack-config/webpack.development.config.js",
    "start-sigc": "webpack-dev-server --config=webpack-config/webpack.development.config.js",
    "start:build": "npm run build && npm run debug:build",
    "test": "npm run karma"
  }
}